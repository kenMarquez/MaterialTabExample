<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bambu.mobile.materialtabexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Tabs1Activity" />
        <activity android:name=".Tabs2Activity" />
        <activity
            android:name=".Tabs3Activity"
            android:label="@string/title_activity_tabs3"
            android:theme="@style/MyMaterialTheme" />
        <activity android:name=".Tabs4Activity"></activity>
    </application>

</manifest>
